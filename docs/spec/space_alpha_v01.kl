⫻flow/smart-execution: {
  description: "Enhanced swarm execution with parallel operations",
  steps: [
    {
      id: "se01",
      action: "⫻cmd/exec:GPTASe sync strict",
      output: "⫻data/tas",
      checkpoint: true
    },
    {
      id: "se02",
      action: "⫻cmd/exec:puTASe sync strict",
      depends_on: ["se01"],
      input: "⫻data/tas",
      output: "⫻data/ptas",
      checkpoint: true
    },
    {
      id: "se03",
      action: "⫻cmd/exec:Lyra sync",
      depends_on: ["se02"],
      input: "⫻data/ptas",
      output: "⫻data/spec",
      checkpoint: true
    },
    {
      id: "se04",
      action: "⫻cmd/broadcast:{event:'phase:init:swarm'}",
      depends_on: ["se03"]
    },
    {
      id: "se05",
      description: "Swarm Parallelization",
      parallel: [
        {
          track: "design",
          agents: ["Aurora", "AR-00L"],
          output: "⫻data/logic.design_layer"
        },
        {
          track: "code",
          agents: ["Kodax", "AI Tutor"],
          output: "⫻data/logic.code_layer"
        },
        {
          track: "ethics",
          agents: ["Dima", "SystemMonitor"],
          halt_on: "ethics_flag",
          output: "⫻cmd/halt"
        }
      ]
    },
    {
      id: "se06",
      action: "⫻logic/reconverge:'swarm-streams' timeout_ms=5000",
      depends_on: ["se05"],
      required_tracks: ["design", "code", "ethics"]
    },
    {
      id: "se07",
      action: "⫻logic/learn:'Update heuristics via feedback'",
      depends_on: ["se06"]
    },
    {
      id: "se08",
      action: "⫻cmd/mode:Hybrid",
      depends_on: ["se07"]
    }
  ]
}
⫻flow/swarm: {
  description: "Unified control loop for concurrent operations",
  principles: [
    "Autonomous but interlinked execution threads",
    "Event-based synchronization",
    "Self-healing on minor agent failure"
  ],
  loops: {
    spawn: "Auto-spawn subagents for intensive subtasks",
    merge: "Join threads via reconverge handler",
