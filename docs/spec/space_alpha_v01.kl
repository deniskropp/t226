    "ReconvergeReady",
    "EthicsBreach",
    "InsightGenerated"
  ]
}
// ============================================
// LOGIC & CONTROL STRUCTURES
// ============================================
⫻logic/error: {
  on_ethics: {
    action: "⫻cmd/halt:ethics reason='ethics_validation_failed'",
    notify: ["Dima", "Orchestrator"],
    severity: "critical"
  },
  on_bias: {
    action: "⫻cmd/halt:safety reason='bias_detected'",
    notify: ["SystemMonitor", "Dima"],
    severity: "high"
  },
  on_conflict: {
    action: "⫻cmd/exec:Halin sync",
    notify: ["Orchestrator"],
    severity: "medium"
  },
  on_timeout: {
    action: "⫻cmd/halt:manual reason='operation_timeout'",
    notify: ["Fizz La Metta"],
    severity: "medium"
  }
}
⫻logic/validate: {
  description: "Integrity verification checkpoint",
  frequency: "per-phase",
  procedure: [
    "1. Audit agent logs in ⫻data/trace",
    "2. Confirm ethical compliance via Dima",
    "3. Verify state consistency",
    "4. Resume or restart flow based on results"
  ],
  checkpoints: {
    ethics: {
      agent: "Dima",
      frequency: "per-phase",
      blocking: true
    },
    bias: {
      agent: "SystemMonitor",
      frequency: "continuous",
      blocking: false
    },
    conflict: {
      agent: "Halin",
      frequency: "on-demand",
      blocking: true
    }
  }
}
⫻logic/retry: {
  max_attempts: 3,
  backoff_ms: [1000, 2000, 4000],
  on_final_failure: "⫻cmd/halt:error reason='max_retries_exceeded'"
}
